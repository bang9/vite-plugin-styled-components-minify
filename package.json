{
  "name": "vite-plugin-styled-components-minify",
  "version": "1.0.0",
  "description": "vite plugin for styled-components minify",
  "author": "bang9",
  "repository": "git@github.com:bang9/vite-plugin-styled-components-minify.git",
  "license": "MIT",
  "keywords": [
    "vite",
    "styled-components",
    "minify"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "test": "vitest",
    "prepare": "pnpm build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "babel-plugin-styled-components": "^2.1.4",
    "magic-string": "^0.30.17",
    "prettier": "^3.4.2",
    "rollup": "^4.31.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "preserve",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always"
  },
  "packageManager": "pnpm@9.15.3"
}
